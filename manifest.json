{
  "name": "Fresh Tomatoes",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Lookup movie ratings w/o going to RM",
  "background" : {
    "page": "background.html"
  },
  "browser_action": {
    "default_title" : "Freshy",
    "default_popup": "home.html"
  },
  "permissions" : [
    "http://api.rottentomatoes.com/api/*"
  ]
}